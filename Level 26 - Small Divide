-- NOTE: This does not qualify for the speed challenge.

a:
    COPYFROM 9
    COPYTO   2
    INBOX   
    COPYTO   0
    INBOX   
    COPYTO   1
    COPYFROM 0
b:
    SUB      1
    COPYTO   0
    JUMPN    c
    BUMPUP   2
    COPYFROM 0
    JUMP     b
c:
    COPYFROM 2
    OUTBOX  
    JUMP     a


