    INBOX   
    COPYTO   0
    INBOX   
    JUMPZ    e
    COPYTO   1
    COPYTO   2
    COPYFROM 0
    JUMPZ    f
b:
    BUMPDN   0
    JUMPZ    d
    JUMPN    c
    COPYFROM 2
    ADD      1
    COPYTO   2
    JUMP     b
c:
d:
    COPYFROM 2
e:
f:
    OUTBOX  
    JUMP     a


