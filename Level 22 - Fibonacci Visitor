-- HUMAN RESOURCE MACHINE PROGRAM --
-- NOTE: This does not qualify for the size challenge.
-- NOTE: This does not qualify for the speed challenge.

    BUMPUP   9
a:
    COPYFROM 9
    COPYTO   0
    COPYTO   1
    INBOX   
    COPYTO   2
b:
    COPYFROM 0
    OUTBOX  
    COPYFROM 1
    COPYTO   5
    ADD      0
    COPYTO   1
    COPYFROM 5
    COPYTO   0
    SUB      2
    JUMPZ    d
    JUMPN    c
    JUMP     e
c:
    JUMP     b
d:
    COPYFROM 2
    OUTBOX  
e:
    JUMP     a


