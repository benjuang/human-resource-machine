    INBOX   
    COPYTO   0
    OUTBOX  
    COPYFROM 14
    COPYTO   1
    COMMENT  0
a:
b:
    COPYFROM 14
    SUB      14
    COPYTO   14
    INBOX   
    COPYTO   13
c:
    SUB      [14]
    JUMPZ    b
    BUMPUP   14
    COPYFROM [14]
    JUMPZ    d
    COPYFROM 13
    JUMP     c
d:
    COPYFROM 13
    COPYTO   [14]
    OUTBOX  
    BUMPUP   14
    COPYFROM 14
    SUB      14
    COPYTO   [14]
    JUMP     a


