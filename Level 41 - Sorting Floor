-- HUMAN RESOURCE MACHINE PROGRAM --
-- NOTE: This does not qualify for the size challenge.
-- NOTE: This does not qualify for the speed challenge.

a:
    COPYFROM 24
    COPYTO   20
    COPYTO   21
b:
    INBOX   
    COPYTO   [20]
    JUMPZ    c
    BUMPUP   20
    JUMP     b
c:
d:
    COPYFROM 24
    COPYTO   20
e:
f:
g:
    COPYFROM [20]
    JUMPZ    i
    COPYTO   22
    BUMPUP   20
    COPYFROM [20]
    JUMPZ    h
    SUB      22
    JUMPN    f
    JUMPZ    g
    BUMPUP   21
    COPYFROM [20]
    COPYTO   23
    COPYFROM 22
    COPYTO   [20]
    BUMPDN   20
    COPYFROM 23
    COPYTO   [20]
    BUMPUP   20
    JUMP     e
h:
i:
    COPYFROM 21
    JUMPZ    j
    SUB      21
    COPYTO   21
    JUMP     d
j:
    COMMENT  0
    BUMPDN   20
k:
    COPYFROM [20]
    OUTBOX  
    BUMPDN   20
    JUMPN    a
    JUMP     k


