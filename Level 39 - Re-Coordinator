a:
    COPYFROM 14
    COPYTO   4
    INBOX   
    COPYTO   0
b:
    SUB      15
    JUMPN    c
    COPYTO   0
    BUMPUP   4
    COPYFROM 0
    JUMP     b
c:
    COPYFROM 0
    OUTBOX  
    COPYFROM 4
    OUTBOX  
    JUMP     a


